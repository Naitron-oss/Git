{
  "name": "js-git-diff test",
  "short_name": "diff-test",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "31",
  "description": "js-git diff test",
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "http://*/*",
    "https://*/*",
    {"socket": [ "tcp-connect", "tcp-listen" ]}
  ],
  "icons": {
    
  }
}